  <h1 class="mt-3 text-center poppins-semibold">History Prediction</h1>
  <div class="container">
    <div class="d-flex mb-3">
      <a href="/initializeModel" class="btn btn-outline-info me-auto">
        Initialize model
      </a>
      <a href="/getDataSaved" class="btn btn-outline-warning">
        See Data Pollution
      </a>
      
    </div>
    
    
    <% if (success_msg) { %>
      <p class="success-alert">
        <%= success_msg %>
      </p>	
    <% } %>
    <% if (error_msg) { %>
      <p class="fail-alert">
        <%= error_msg %>
      </p>
    <% } %>
    <% if (predictions.length > 0) { %>
      <table class="table table-striped">
        <thead>
          <tr>
            <th scope="col">#</th>
            <th scope="col">Timestamp</th>
            <th scope="col">Jenis</th>
            <th scope="col">Value</th>
          </tr>
        </thead>
        <tbody>
          <% predictions.forEach((prediction, idx) => { %>
          <tr>
            <td><%= idx + 1 %></td>
            <td><%= new Date(prediction.timestamp).toLocaleString() %></td>
            <td><%= prediction.jenis %></td>
            <td><%= prediction.value.toFixed(2) %></td>
          </tr>
        <% }) %>
        </tbody>
      </table>
    <% } else { %>
      <p>No prediction results available.</p>
    <% } %>
    
    
</div>
