<!DOCTYPE html>
<html>
<head>
  <title>Air Pollution Prediction</title>
</head>
<body>
  <h1>Air Pollution Prediction</h1>
  <% if (predictions.length > 0) { %>
    <table class="table table-striped">
      <thead>
        <tr>
          <th scope="col">#</th>
          <th scope="col">Timestamp</th>
          <th scope="col">Jenis</th>
          <th scope="col">Value</th>
        </tr>
      </thead>
      <tbody>
        <% predictions.forEach((prediction, idx) => { %>
        <tr>
          <td><%= idx + 1 %></td>
          <td><%= new Date(prediction.timestamp).toLocaleString() %></td>
          <td><%= prediction.jenis %></td>
          <td><%= prediction.value.toFixed(2) %></td>
        </tr>
      <% }) %>
      </tbody>
    </table>
  <% } else { %>
    <p>No prediction results available.</p>
  <% } %>
</body>
</html>